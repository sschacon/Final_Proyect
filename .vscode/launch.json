{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run FinalProject-HTML5Project",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/finalproject-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/logaligroupHTML5Project/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/HTML5Project",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/FinalProject/HTML5Project\"}"
      },
      "envFile": "${workspaceFolder}/HTML5Project/.env1",
      "preLaunchTask": "Build FinalProject"
    },
